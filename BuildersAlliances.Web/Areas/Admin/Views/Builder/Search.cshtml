
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                Search  Builder
            </header>
            <div class="panel-body">

             
                    <div class="row">
                        <div class="col-md-4">
                            <label>Builder Name</label>
                        </div>

                        <div class="col-md-4">
                            <label>Email</label>
                        </div>

                        <div class="col-md-4">
                            <label>Phone</label>
                        </div>
                        <div>

                        </div>
                    </div>
                        <div class="row">
                            <div class="col-md-4">
                                <input class="form-control FilterSearch" id="txtBuilder" placeholder="Manufacturer" type="text" name="FilterSearch">
                              
                            </div>
                            <div class="col-md-4">
                                <input class="form-control FilterSearch" id="txtEmail" placeholder="Address" type="text" name="FilterSearch">
                            </div>
                            <div class="col-md-4">
                              
                                <input class="form-control FilterSearch" id="txtPhone" placeholder="Contact" type="text" name="FilterSearch">
                            </div>


                        </div>

                            <div class="row">
                                <div class="col-md-4">
                                    <label>Address1</label>
                                </div>

                                <div class="col-md-4">
                                    <label>Address2</label>
                                </div>
                                <div class="col-md-4">
                                    <label>Address3</label>
                                </div>


                                <div>

                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                              
                                    <input class="form-control FilterSearch" id="txtAddress1" placeholder="Email" type="text" name="FilterSearch">
                                </div>
                                <div class="col-md-4">
                                    <input class="form-control FilterSearch" id="txtAddress2" placeholder="WebSiteUrl" type="text" name="FilterSearch">
                                </div>
                                <div class="col-md-4">
                                    <input class="form-control FilterSearch" id="txtAddress3" placeholder="WebSiteUrl" type="text" name="FilterSearch">
                                </div>



                            </div>



                            <div class="row">
                                <div class="pull-right btnsetting">
                                    <button type="button" id="btnSearch" class="btn btn-info">Search</button>
                                    <button type="button" onclick="Close();" class="btn btn-cancel">Cancel</button>
                                </div>
                            </div>

                               
              
            </div>
        </section>
    </div>
</div>