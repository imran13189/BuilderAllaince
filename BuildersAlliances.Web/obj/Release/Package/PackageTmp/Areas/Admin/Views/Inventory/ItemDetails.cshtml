@model BuildersAlliances.CustomModel.ItemModel
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                Item Detail
            </header>
            <div class="panel-body">

            
                    <div class="row">
                        <div class="col-md-4">
                            <label>Item Name</label>
                        </div>

                        <div class="col-md-4">
                            <label>Item Color</label>
                        </div>

                        <div class="col-md-4">
                            <label>Manufacturer</label>
                        </div>
                        <div>

                        </div>
                    </div>
                        <div class="row">
                            <div class="col-md-4">
                             @Model.ItemName
                            </div>
                            <div class="col-md-4">
                                @Model.ColorName
                                
                            </div>
                            <div class="col-md-4">
                               @Model.ManufacturerName

                            </div>


                        </div>

                            <div class="row">
                               

                                <div class="col-md-4">
                                    <label>List Price</label>
                                </div>

                                <div class="col-md-4">
                                    <label>Cubes</label>
                                </div>

                                <div class="col-md-4">
                                    <label>Item SKU</label>
                                </div>
                                <div>

                                </div>
                            </div>
                            <div class="row">
                               
                                <div class="col-md-4">
                                    @Model.ListPrice 
                                </div>
                                <div class="col-md-4">
                                  @Model.Cubes
                                </div>

                                <div class="col-md-4">
                                    @Model.ItemSKU
                                </div>


                            </div>

                   

                         

                
                

            </div>
        </section>
    </div>
</div>

