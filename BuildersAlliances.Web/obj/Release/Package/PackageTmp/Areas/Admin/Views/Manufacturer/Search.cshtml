
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                Search  Manufacturer
            </header>
            <div class="panel-body">

             
                    <div class="row">
                        <div class="col-md-4">
                            <label>Manufacturer Name</label>
                        </div>

                        <div class="col-md-4">
                            <label>Address</label>
                        </div>

                        <div class="col-md-4">
                            <label>Contact</label>
                        </div>
                        <div>

                        </div>
                    </div>
                        <div class="row">
                            <div class="col-md-4">
                                <input class="form-control FilterSearch" id="txtManufacturerName" placeholder="Manufacturer" type="text" name="FilterSearch">
                              
                            </div>
                            <div class="col-md-4">
                                <input class="form-control FilterSearch" id="txtAddress" placeholder="Address" type="text" name="FilterSearch">
                            </div>
                            <div class="col-md-4">
                              
                                <input class="form-control FilterSearch" id="txtContactNumber" placeholder="Contact" type="text" name="FilterSearch">
                            </div>


                        </div>

                            <div class="row">
                                <div class="col-md-4">
                                    <label>Email</label>
                                </div>

                                <div class="col-md-4">
                                    <label>Web site url</label>
                                </div>


                                <div>

                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                              
                                    <input class="form-control FilterSearch" id="txtEmailId" placeholder="Email" type="text" name="FilterSearch">
                                </div>
                                <div class="col-md-4">
                                    <input class="form-control FilterSearch" id="txtWebSiteUrl" placeholder="WebSiteUrl" type="text" name="FilterSearch">
                                </div>



                            </div>



                            <div class="row">
                                <div class="pull-right btnsetting">
                                    <button type="button" id="btnSearch" class="btn btn-info">Search</button>
                                    <button type="button" onclick="Close();" class="btn btn-cancel">Cancel</button>
                                </div>
                            </div>

                               
              
            </div>
        </section>
    </div>
</div>